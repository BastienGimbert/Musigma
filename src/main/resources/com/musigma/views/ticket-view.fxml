<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="697.0" prefWidth="1058.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.musigma.controllers.workspaces.TicketController">
    <children>
        <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="1058.0" spacing="10">
            <children>
                <TextField fx:id="textFieldType" promptText="ex : VIP">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </TextField>
                <TextField fx:id="textFieldQuantite" promptText="ex : 100" />
                <TextField fx:id="textFieldPrix" promptText="ex : 10.99" />
                <Button fx:id="ajouterButton" mnemonicParsing="false" text="Ajouter" />

            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <padding>
                <Insets left="10.0" />
            </padding>
            <VBox.margin>
                <Insets />
            </VBox.margin></HBox>
      <TabPane fx:id="tabPane" prefHeight="648.0" prefWidth="1058.0" tabClosingPolicy="UNAVAILABLE" />
    </children>
</VBox>
